## Add this filter to .git/config to strip notebook outputs from commits
# [filter "strip-notebook-output"]
#         clean = "~/micromamba/envs/poly/bin/python -m nbconvert --ClearOutputPreprocessor.enabled=True --ClearMetadataPreprocessor.enabled=True --to=notebook --stdin --stdout --log-level=ERROR"

## Then, we add this filter here in .gitattributes:
*.ipynb filter=strip-notebook-output