## Add this filter to .git/config to strip notebook outputs from commits
# [filter "strip-notebook-output"]
        # clean = "~/PATH/TO/YOUR/python -m jupyter nbconvert --ClearOutputPreprocessor.enabled=True --ClearMetadataPreprocessor.enabled=True --to=notebook --stdin --stdout --log-level=ERROR"

## Then, uncomment this line:
# *.ipynb filter=strip-notebook-output